package GreatestNumber;

public class Assignment {
	
	public static void main(String args[]) {
		
		int num1 =700;
		int num2=900;
		int num3=600;
		
		if(num1>num2) {
			
			if(num1>num3)
				System.out.println(num1);
			
			else
				
				System.out.print(num3);
		}
		else 
		{
			 
			if(num2>num3)
				System.out.println(num2);
			
			else
				 System.out.println(num3);
			
		}
		
	}

}
